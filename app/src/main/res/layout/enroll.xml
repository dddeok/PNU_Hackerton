<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@+id/programView"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/border"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="13dp">
            <ImageButton
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:scaleType="fitXY"
                android:layout_marginRight="13dp"
                android:background="@drawable/close_big"
                android:id="@+id/close"
                android:layout_gravity="right"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#000000"
                android:lineSpacingExtra="6sp"
                android:layout_marginLeft="11dp"
                android:text="다꿈 프로젝트"
                android:id="@+id/programName"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="11dp"
            android:background="#000000"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="9sp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6sp"
                android:text="2018-08-03"
                android:id="@+id/start"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="9sp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6sp"
                android:text="~"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="9sp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6sp"
                android:text="2018-08-04"
                android:id="@+id/end"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="활동기관   l    "
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="부산대학교"
                android:id="@+id/place"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="교육기간   l    "
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="3시간"
                android:id="@+id/date"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="관리기관   l    "
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="아두이노 교육"
                android:id="@+id/check_place"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="담당자   l    "
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#de000000"
                android:lineSpacingExtra="5sp"
                android:layout_gravity="center"
                android:text="공미정"
                android:id="@+id/manager"/>
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="내용이 들어가는 부분입니다"
            android:textColor="#de000000"
            android:padding="5dp"
            android:lineSpacingExtra="5sp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="20dp"
            android:id="@+id/text"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="10dp">
            <ImageView
                android:layout_width="18.6dp"
                android:layout_height="18.6dp"
                android:scaleType="fitXY"
                android:background="@drawable/location"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="13sp"
                android:textColor="#000000"
                android:lineSpacingExtra="6sp"
                android:text="위치"
                />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:layout_marginBottom="20dp"
            android:padding="0dp"
            android:background="#f5f5f5"
            android:id="@+id/map_view"></RelativeLayout>

        <ImageButton
            android:layout_width="87dp"
            android:layout_height="31dp"
            android:background="@drawable/enroll"
            android:scaleType="fitXY"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="30dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginBottom="28.5dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:textColor="#000000"
                android:text="관심글 추가"
                android:layout_gravity="center"
                />
            <ImageButton
                android:layout_width="22dp"
                android:layout_height="21dp"
                android:scaleType="fitXY"
                android:background="@drawable/emptystar"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="14dp"
                android:id="@+id/favorite"/>
        </LinearLayout>

    </LinearLayout>

</ScrollView>