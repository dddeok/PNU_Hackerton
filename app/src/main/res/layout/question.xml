<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="457dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/border"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="13dp">
            <ImageButton
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:scaleType="fitXY"
                android:layout_marginRight="13dp"
                android:background="@drawable/close_big"
                android:id="@+id/close"
                android:layout_gravity="right"
                />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:lineSpacingExtra="6sp"
                    android:layout_marginLeft="11dp"
                    android:text="1:1 문의"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="11dp"
            android:background="#000000"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="20dp"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="9sp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6sp"
                android:text="고객문의는 순차적으로 처리되니 양해부탁드립니다"
                android:id="@+id/start"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="38dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:textColor="#000000"
                android:lineSpacingExtra="5sp"
                android:text="이름"
                android:layout_gravity="center"
                />
            <EditText
                android:layout_width="0dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:layout_marginLeft="35.7dp"
                android:id="@+id/name"
                android:background="@drawable/border"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="38dp"
            android:layout_marginTop="8dp">
            <TextView
                android:layout_width="28dp"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:textColor="#000000"
                android:lineSpacingExtra="5sp"
                android:text="연락처"
                android:layout_gravity="center"
                />
            <EditText
                android:layout_width="0dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:layout_marginLeft="25.7dp"
                android:id="@+id/tel"
                android:background="@drawable/border"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="38dp"
            android:layout_marginTop="8dp">
            <TextView
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:textColor="#000000"
                android:lineSpacingExtra="5sp"
                android:text="E-mail"
                android:layout_gravity="center"
                />
            <EditText
                android:layout_width="0dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:layout_marginLeft="21.7dp"
                android:id="@+id/email"
                android:background="@drawable/border"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="38dp"
            android:layout_marginTop="8dp">

            <TextView
                android:layout_width="37dp"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="5sp"
                android:text="문의사항"
                android:textColor="#000000"
                android:textSize="10sp"
                android:layout_gravity="left"/>
            <EditText
                android:layout_width="0dp"
                android:layout_height="69.2dp"
                android:layout_weight="1"
                android:layout_marginLeft="16.7dp"
                android:id="@+id/text"
                android:background="@drawable/border"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="38dp"
            android:layout_marginTop="8dp">

            <TextView
                android:layout_width="37dp"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="5sp"
                android:text="파일첨부"
                android:textColor="#000000"
                android:textSize="10sp"
                android:layout_gravity="left"/>

            <Button
                android:id="@+id/file"
                android:layout_width="0dp"
                android:layout_height="67.4dp"
                android:layout_marginLeft="16.7dp"
                android:layout_weight="1"
                android:background="@drawable/border" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"></View>
            <ImageButton
                android:layout_width="87dp"
                android:layout_height="31dp"
                android:layout_marginBottom="57.9dp"
                android:scaleType="fitXY"
                android:background="@drawable/send"
                android:layout_gravity="center"
                android:id="@+id/send"/>
    </LinearLayout>

</LinearLayout>